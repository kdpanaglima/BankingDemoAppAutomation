<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Customer Access Scenarios" parallel="methods" thread-count="4">

	<listeners>

		<listener class-name="com.parasoft.banking.listeners.CustomListeners" />

		<listener class-name="com.parasoft.banking.errorcollectors.TestListenerAdapter" />
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />

	</listeners>

	<test name="Customer Login" parallel="methods">
		<classes>
			<class name="com.parasoft.banking.testcases.VerifyCustomerLogin" />
		<class name="com.parasoft.banking.testcases.VerifyCustomerRegistration" />

		</classes>
	</test>




</suite> <!-- Suite -->